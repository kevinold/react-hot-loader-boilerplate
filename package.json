{
  "name": "react-hot-loader-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "node server.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "eslint src",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && babel-node tools/buildHtml.js",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js src/**/*.spec.js --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:leshow/react-hot-loader-boilerplate.git"
  },
  "author": "Evan Cameron",
  "license": "MIT",
  "homepage": "https://github.com:leshow/react-hot-loader-boilerplate.git",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-register": "^6.8.0",
    "browser-sync": "^2.12.7",
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "colors": "^1.1.2",
    "connect-history-api-fallback": "^1.2.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "npm-run-all": "^1.8.0",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
